<html class="no-js" lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <title>Open Food Data about Actors, Products and Services</title>
        <link rel="stylesheet" href="https://openfooddata.github.io/actors/css/foundation.min.css"/>
    </head>
    <body>

    <!-- Start Top Bar -->
    <div class="top-bar">
        <div class="top-bar-left">
            <ul class="menu">
                <li class="menu-text">Open Food Data Actors</li>
                <li><a href="https://openfooddata.github.io/actors/list0.html">List</a></li>
                <li><a href="https://openfooddata.github.io/actors/map.html">Map</a></li>
            </ul>
        </div>
        <div class="top-bar-right">
            <ul class="menu">
                <li><a href="http://openfooddata.github.io/">Main OFD Page</a></li>
                <li><a href="https://github.com/openfooddata/actors">GitHub</a></li>
                <li><a href="https://w3id.org/openfooddata/onto/core">OFD Ontology</a></li>
                <li><a href="https://github.com/openfooddata/vocab">OFD Vocabs</a></li>
            </ul>
        </div>
    </div>
    <!-- End Top Bar -->
    <div class="callout large">
        <div class="row columns text-center"><h3>Stats</h3></div>
        <div class="row">
            <div class="medium-6 columns">
                {{#country}}
                    <div class="row">
                        <div class="medium-6 columns">{{name}}</div>
                        <div class="medium-6 columns">{{count}}</div>
                    </div>
                {{/country}}
            </div>
            <div class="medium-6 columns">
                {{#actortype}}
                    <div class="row">
                        <div class="medium-6 columns"><a class="actortype" href="#">{{name}}</a></div>
                        <div class="medium-6 columns">{{count}}</div>
                    </div>
                {{/actortype}}
            </div>
        </div>
    </div>
<hr>
    <div class="row columns text-center"><h3>Downloads</h3></div>
    <div class="row">
            <div class="column">
                <li><a href="https://openfooddata.github.io/actors/_all.geojson">GeoJSON</a></li>
                <li><a href="https://openfooddata.github.io/actors/_all.rdf">RDF-XML</a></li>
                <li><a href="https://openfooddata.github.io/actors/_all.ttl">TTL</a></li>
            </div>
    </div>


    <script
            src="https://code.jquery.com/jquery-2.2.4.min.js"
            integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44="
            crossorigin="anonymous"></script>
    <script src="https://openfooddata.github.io/actors/js/foundation.min.js"></script>
    <script>
        $(document).foundation();
    </script>
    <script src="https://openfooddata.github.io/actors/js/adr.js"></script>
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-102767097-1', 'auto');
        ga('send', 'pageview');

    </script>
    </body>
    </html>